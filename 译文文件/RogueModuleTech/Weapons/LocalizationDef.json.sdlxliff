&lt;b&gt;&lt;color=#ffcc00&gt;使用RAC5弹药&lt;/color&gt;&lt;/b&gt;</mrk> </target><sdl:seg-defs><sdl:seg id="423" conf="Translated" origin="auto-propagated" origin-system="从句段 417 沿用" percent="100"><sdl:rep id="Tg60vOHJzWtq7w5QG7fNcauLNuI="/><sdl:prev-origin origin="auto-propagated" origin-system="从句段 417 沿用" percent="100"><sdl:rep id="Tg60vOHJzWtq7w5QG7fNcauLNuI="/><sdl:prev-origin/><sdl:value key="created_by">FATE-MOON\zly</sdl:value><sdl:value key="created_on">08/28/2022 17:35:50</sdl:value><sdl:value key="modified_on">09/10/2022 05:28:28</sdl:value><sdl:value key="last_modified_by">FATE-MOON\zly</sdl:value><sdl:value key="SDL:OriginalTranslationHash">-2058757258</sdl:value><sdl:value key="SegmentIdentityHash">Tg60vOHJzWtq7w5QG7fNcauLNuI=</sdl:value></sdl:prev-origin><sdl:value key="created_by">FATE-MOON\zly</sdl:value><sdl:value key="created_on">08/28/2022 17:35:50</sdl:value><sdl:value key="modified_on">09/12/2022 23:02:47</sdl:value><sdl:value key="last_modified_by">FATE-MOON\zly</sdl:value><sdl:value key="SDL:OriginalTranslationHash">1514115434</sdl:value><sdl:value key="SegmentIdentityHash">Tg60vOHJzWtq7w5QG7fNcauLNuI=</sdl:value></sdl:seg></sdl:seg-defs></trans-unit></group><group><sdl:cxts><sdl:cxt id="424"/></sdl:cxts><trans-unit id="95588f0a-c273-4a59-b0cb-fb6d01add7bc"><source>Rotary AC/5</source><seg-source><mrk mtype="seg" mid="424">Rotary AC/5</mrk></seg-source><target><mrk mtype="seg" mid="424">转轮AC/5</mrk></target><sdl:seg-defs><sdl:seg id="424" conf="Translated" origin="auto-propagated" origin-system="从句段 418 沿用" percent="100"><sdl:rep id="lc5/rkU0OAVotWn5jXJowfudhfw="/><sdl:value key="created_by">FATE-MOON\zly</sdl:value><sdl:value key="created_on">08/26/2022 06:24:35</sdl:value><sdl:value key="modified_on">09/10/2022 05:28:28</sdl:value><sdl:value key="last_modified_by">FATE-MOON\zly</sdl:value><sdl:value key="SDL:OriginalTranslationHash">920503143</sdl:value><sdl:value key="SegmentIdentityHash">lc5/rkU0OAVotWn5jXJowfudhfw=</sdl:value></sdl:seg></sd